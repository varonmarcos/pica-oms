<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    
    <navigation-rule>
		<from-view-id>/LoginPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>exito</from-outcome>
			<to-view-id>/pages/PrincipalPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/PrincipalPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goToProductos</from-outcome>
			<to-view-id>/pages/ProductosPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	

</faces-config>
